# store local T&D capacity within each zone
let output_section := 'local_trans_and_distribution_capacity';
let outfile := result_header_paths[output_section];
printf "scenario_id	carbon_cost	period	load_area_id	load_area	local_td_mw	fixed_cost\n" > (outfile);
close (outfile);

let outfile := sprintf( result_file_path_templates[output_section], carbon_cost );
printf {p in PERIODS, a in LOAD_AREAS}:
  "%s	%.2f	%d	%d	%s	%.2f	%.2f\n",
  scenario_id, carbon_cost, p, load_area_id[a], a, 
  sum {install_yr in PERIODS: install_yr <= p < transmission_end_year[install_yr]} InstallLocalTD[a, install_yr],
  (sum {install_yr in PERIODS: install_yr <= p < transmission_end_year[install_yr]} InstallLocalTD[a, install_yr])
    * local_td_annual_payment_per_mw * (1-(1/(1+discount_rate)^(num_years_per_period)))/discount_rate
  > (outfile);

close (outfile);

printf "%d seconds to store results.\n", time()-curtime;
